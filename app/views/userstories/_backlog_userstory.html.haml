%tr{ class: userstory.status }
  %td.move= sprint_link(userstory) if userstory.position == 'backlog'
  %td.category= "#{userstory.category_name}"
  %td.description= link_to "#{userstory.description}", [@project, userstory]
  %td.priority
  %td.velocity
  %td.status= "#{userstory.status}"
  %td.name= "#{userstory.name}"
