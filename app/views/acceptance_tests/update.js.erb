$('li#acceptance_test_<%= @acceptance_test.id%>').replaceWith('<%= j render @acceptance_test %>');
$('#edit_acceptance_test_<%= @acceptance_test.id %>').remove();
<% if @acceptance_test.complete? %>
  $('#acceptance_test_<%= @acceptance_test.id %>').appendTo('section#acceptance_tests section.complete ul');
<% else %>
  $('#acceptance_test_<%= @acceptance_test.id %>').appendTo('section#acceptance_tests section.incomplete ul');
<% end %>
$('li').show();
